          <div class="row">
            <div class="col-sm-12">
              <div class="d-lg-flex align-items-center">
                <!-- logo -->
                <a class="navbar-brand logo" href="index.html">
                  <img src="images/InsImg.gif" alt="Logo">
                </a>
                <nav class="navbar navbar-expand-lg">

